{
  "permissions": {
    "allow": [
      "Bash(mv:*)",
      "Bash(rmdir:*)",
      "Bash(true)",
      "Bash(npm run build:*)",
      "Bash(ls:*)",
      "Bash(test:*)",
      "Bash(cat:*)",
      "Bash(git add:*)",
      "Bash(git rebase:*)",
      "Bash(git merge-base:*)",
      "Bash(git merge:*)",
      "Bash(git commit:*)",
      "Read(~/.zshrc)"
    ],
    "deny": [
      "Bash(curl:*)",
      "Bash(npm run dev:*)",
      "Bash(op: *)",
      "Bash(git push:*)",
      "Bash(rm -rf: *)"
    ]
  }
}
